    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "html-webpack-plugin": "^5.6.5",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
